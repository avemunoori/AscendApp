{
  "expo": {
    "name": "Ascend",
    "slug": "ascend-climbing-app",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "ascend",
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.ascendclimbing.app",
      "buildNumber": "9",
      "icon": {
        "image": "./assets/icon.png",
        "resizeMode": "contain"
      },
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "CFBundleDisplayName": "Ascend",
        "CFBundleIconName": "AppIcon",
        "NSCameraUsageDescription": "This app does not use camera access.",
        "NSPhotoLibraryUsageDescription": "This app does not use photo library access.",
        "NSLocationWhenInUseUsageDescription": "This app does not use location services."
      },
      "appStoreUrl": "https://apps.apple.com/app/ascend-climbing-app/id1234567890"
    },
    "android": {
      "package": "com.ascendclimbing.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "INTERNET"
      ],
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.ascendclimbing.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "650c0d0b-6ea6-43ce-a72a-d7e4afdc5795"
      }
    },
    "owner": "avemunoori",
    "description": "Track your climbing progress, manage training plans, and analyze performance across bouldering, lead, and top rope climbing. Beautiful glass morphism design with comprehensive analytics.",
    "primaryColor": "#764ba2"
  }
}
